let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/repos/dealercenter-digital/bc-desktop-client
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
set shortmess+=aoO
badd +1 ~/repos/dealercenter-digital/bc-desktop-client/src/client/commons/components/ProductTitle/ProductTitle.tsx
badd +69 ~/repos/dealercenter-digital/bc-desktop-client/src/client/commons/components/TabBarPageLayout/TabBarPageLayout.scss
