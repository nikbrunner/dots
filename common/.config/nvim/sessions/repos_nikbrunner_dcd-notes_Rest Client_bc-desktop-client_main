let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/repos/nikbrunner/dcd-notes/Rest\ Client/bc-desktop-client
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +41 ~/repos/nikbrunner/dcd-notes/Rest\ Client/bc-desktop-client/env.http
argglobal
%argdel
$argadd ./
edit ~/repos/nikbrunner/dcd-notes/Rest\ Client/bc-desktop-client/env.http
argglobal
setlocal keymap=
setlocal noarabic
