{
  "HTML Comment": {
    "prefix": "comment",
    "body": "<!-- $1 -->$0",
    "description": "Markdown HTML comment"
  },
  "Code block": {
    "prefix": "code",
    "body": ["```$1", "$0", "```"],
    "description": "Markdown code block"
  },
  "JavaScript code block": {
    "prefix": "code-js",
    "body": ["```javascript", "$0", "```"],
    "description": "Markdown JavaScript code block"
  },
  "TypeScript code block": {
    "prefix": "code-ts",
    "body": ["```typescript", "$0", "```"],
    "description": "Markdown TypeScript code block"
  },
  "Lua code block": {
    "prefix": "code-lua",
    "body": ["```lua", "$0", "```"],
    "description": "Markdown Lua code block"
  },
  "Bash code block": {
    "prefix": "code-bash",
    "body": ["```bash", "$0", "```"],
    "description": "Markdown Bash code block"
  }
}
