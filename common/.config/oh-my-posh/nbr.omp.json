{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "git-foreground": "#193549",
    "git": "#FFFB38",
    "git-modified": "#FF9248",
    "git-diverged": "#FF4500",
    "git-ahead": "#B388FF",
    "git-behind": "#B388FF",
    "red": "#FF0000",
    "green": "#27ae60",
    "blue": "#0000FF",
    "white": "#FFFFFF",
    "black": "#111111"
  },
  "console_title_template": "{{ .UserName }}@{{ .Folder }} :: {{if .Root}}Admin{{end}}",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "plain",
          "properties": {
            "style": "folder"
          },
          "template": "<d>{{ .Parent }}</d>{{ .Path }} "
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:git-foreground",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "template": "({{ .HEAD}})",
          "properties": {
            "fetch_status": true,
            "branch_icon": "\ue725 "
          }
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "#ff0000",
          "template": " \uf00d"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "green",
          "template": "\ue718 {{ .Full }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "blue",
          "template": "\uFCD1 {{ .Full}}"
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "lightMagenta",
          "properties": {
            "command": "git log --pretty=format:%cr -1 || date +%H:%M:%S",
            "shell": "bash"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightYellow",
          "style": "plain",
          "template": "",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "blue",
    "template": ""
  },
  "final_space": true,
  "version": 3
}
