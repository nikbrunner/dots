  "tmux_layouts_dir": "~/.config/tmux/layouts",
  "ensure_cloned": [
    "git@github.com:nikbrunner/dots",
    "git@github.com:nikbrunner/notes",
    "git@github.com:nikbrunner/dcd-notes",
    "git@github.com:nikbrunner/the-black-atom",
    "git@github.com:nikbrunner/koyo",
    "git@github.com:nikbrunner/nbr.haus",
    "git@github.com:nikbrunner/fff-snacks.nvim",
    "git@github.com:nikbrunner/wallpapers",
    { "url": "git@github.com:black-atom-industries/helm", "postClone": "make install" },
    { "url": "git@github.com:nikbrunner/bm", "postClone": "make install" },
    { "url": "git@github.com:nikbrunner/tmux-file-picker", "postClone": "make install" },
